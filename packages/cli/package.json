{
  "name": "dot-ui",
  "version": "0.1.0",
  "description": "CLI for installing Polkadot UI components with automatic API setup",
  "main": "bin/dot-ui.js",
  "bin": {
    "dot-ui": "./bin/dot-ui.js"
  },
  "files": [
    "bin/**/*",
    "lib/**/*",
    "README.md"
  ],
  "scripts": {
    "dev": "node bin/dot-ui.js",
    "build": "echo 'No build step needed'",
    "prepublishOnly": "echo 'Ready to publish'"
  },
  "keywords": [
    "polkadot",
    "web3",
    "ui",
    "components",
    "cli",
    "substrate"
  ],
  "author": "Polkadot UI Initiative",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Polkadot-UI-Initiative/dot-ui.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/Polkadot-UI-Initiative/dot-ui",
  "bugs": "https://github.com/Polkadot-UI-Initiative/dot-ui/issues",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6",
    "execa": "^5.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "preferGlobal": false
}
